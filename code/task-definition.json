{
    "family": "despliegue_web",
    "containerDefinitions": [
        {
            "name": "aplicacion",
            "image": "484992678894.dkr.ecr.eu-south-2.amazonaws.com/repo-footpassion:1",
            "cpu": 0,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [],
            "environmentFiles": [],
            "mountPoints": [
                {
                    "sourceVolume": "fotos",
                    "containerPath": "/var/www/html/img",
                    "readOnly": false
                }
            ],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-create-group": "true",
                    "awslogs-group": "/ecs/despliegue_web",
                    "awslogs-region": "eu-south-2",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "taskRoleArn": "arn:aws:iam::484992678894:role/ecsTaskExecutionRole",
    "executionRoleArn": "arn:aws:iam::484992678894:role/ecsTaskExecutionRole",
    "networkMode": "awsvpc",
    "volumes": [
        {
            "name": "fotos",
            "efsVolumeConfiguration": {
                "fileSystemId": "fs-095ef927774d7b6c2",
                "rootDirectory": "/"
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "1024",
    "memory": "2048",
    "runtimePlatform": {
        "cpuArchitecture": "X86_64",
        "operatingSystemFamily": "LINUX"
    }
}
